{
  "name": "home-money",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startProd": "ng serve --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "watchProd": "ng build --watch --configuration=production",
    "test": "ng test",
    "server": "json-server --watch db.json --port 3000",
    "serverProd": "json-server --watch db3030.json --port 3030",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\"",
    "prod": "concurrently --kill-others \"npm run startProd\" \"npm run serverProd\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^12.2.9",
